GUI local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BlakeScripthub", "SentinelTheme")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
   
MainSection:NewButton("Dupe", "wait  30 secs after clicking button", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Project-WD/SCRIPTHUB/main/Main.Lua"))()
end)


--Credits
local Credits = Window:NewTab("Credits")
